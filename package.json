{
	"name": "product-feedback-backend",
	"version": "1.0.0",
	"description": "This will be the back end to a product feedback application ",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node build/server.js",
		"poststart": "node build/server.js",
		"postinstall": "tsc",
		"watch": "tsc --watch",
		"dev": "nodemon ./src/server.ts",
		"migrate": "knex migrate:latest --knexfile build/config/db/knexfile.js",
		"seed": "knex seed:run --knexfile build/config/db/knexfile.js"
	},
	"author": "Jose De La Cruz",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"core-util-is": "^1.0.3",
		"dotenv": "^10.0.0",
		"express": "^2.5.11",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.95.10",
		"nodemon": "^2.0.13",
		"pg": "^8.7.1",
		"ts-node": "^10.2.1",
		"uuid": "^8.3.2"
	},
	"engines": {
		"node": "14.x",
		"npm": "8.x"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.1",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/node": "^16.10.9",
		"@types/uuid": "^8.3.1",
		"jest": "^27.1.0",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.5",
		"tsc": "^2.0.3"
	}
}
