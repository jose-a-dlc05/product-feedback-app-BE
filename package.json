{
	"name": "product-feedback-backend",
	"version": "1.0.0",
	"description": "This will be the back end to a product feedback application ",
	"main": "server.js",
	"scripts": {
		"start": "node dist/index.js",
		"postinstall": "tsc",
		"watch-node": "nodemon dist/index.js",
		"watch-ts": "tsc -w",
		"dev": "nodemon ./src/server.ts",
		"migrate": "knex migrate:latest --knexfile dist/config/db/knexfile.js",
		"seed": "knex seed:run --knexfile dist/config/db/knexfile.js"
	},
	"author": "Jose De La Cruz",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^16.7.10",
		"bcrypt": "^5.0.1",
		"core-util-is": "^1.0.3",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.95.10",
		"nodemon": "^2.0.13",
		"pg": "^8.7.1",
		"ts-node": "^10.2.1",
		"uuid": "^8.3.2"
	},
	"engines": {
		"node": "12.16.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.1",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/uuid": "^8.3.1",
		"jest": "^27.1.0",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.5",
		"typescript": "^4.4.4"
	}
}
